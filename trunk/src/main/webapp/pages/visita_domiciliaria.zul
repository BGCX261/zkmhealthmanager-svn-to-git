<style>
		body ,table{ margin:0; padding:0;
		} .z-fieldset{ -moz-border-radius:5px; border-radius: 5px;
		-webkit-border-radius: 5px; } .combobox{ font-size:12px;
		border: 1px solid silver; }
		tr.z-row .z-cell{ background-image:none; }
		tr.z-row td.z-row-hover{ background-image:none; }
		.GridSinBorde tr.z-row td.z-row-inner, tr.z-row
			.z-cell,div.z-grid { border: none; overflow: hidden;
			zoom: 1; border-top: none; border-left: none;
			border-right: none; border-bottom: none; }
		</style>
		
		
		<!-- **************** -->
		<!-- Zona de Consulta -->
		<!-- **************** -->
		<groupbox id="groupboxConsulta" visible="false" closable="false"
			mold="3d" height="90%">
			<caption label="Consultar Historias Clínicas "
				style="font-weight: bold">
				<toolbarbutton image="/images/add_perfil.png"
					style="font-weight:bold;float:right;margin-right:50px"
					label="PACIENTE ADMISIONADO"
					id="toolbarbuttonPaciente_admisionado1" />
			</caption>

			<textbox id="tbxEstado" value="" visible="false" />
			<textbox id="tbxId_plan" value="" visible="false" />
			<listbox id="lbxAtendida" mold="select" sclass="combobox"
				width="150px" visible="false" />

			<separator />

			<!-- ***************************** -->
			<!--  Resultado Consulta Maestros  -->
			<!-- ***************************** -->
			<grid id="gridResultado" height="90%" mold="paging" pageSize="20"  vflex="1">
			<auxhead>
						<auxheader colspan="3">
								<hbox align="center">
										<label value="Criterios de B&#250;squeda :" />
										<space />
										<listbox id="lbxParameter" mold="select"
												sclass="combobox" width="150px" />
										<separator />
										<textbox id="tbxValue" width="120px"
												onOK="formVisita_domiciliaria.buscarDatos();"
												style="text-transform:uppercase" />
										<space />

											<label value="Tipo" />
											<space width="5px" />
											<listbox id="lbxTipo_historia" mold="select"
												width="120px"
												onSelect="formVisita_domiciliaria.buscarDatos()">
												<listitem label="Todas" selected="true"
													value="0" />
												<listitem label="Primera Vez" value="1" />
												<listitem label="Control" value="2" />
											</listbox>
											
											<space />
											<toolbarbutton label="Consultar"
												image="/images/Magnifier.gif"
												onClick="formVisita_domiciliaria.buscarDatos();" 
												tooltiptext="Consultar Visita domiciliaria" 
												/>
											<space width="5px" />
											<button mold="trendy"
												image="/images/New16.gif"
												onClick='formVisita_domiciliaria.accionForm(true,"registrar");'
												tooltiptext="Nuevo" visible="false" />	
												
								</hbox>
						</auxheader>
				</auxhead>
				<columns sizable="true">
					<column label="Código historia" width="150px" />
					<column label="Identificación" width="200px" />
					<column label="Nombre" />
					<column label="Fecha" width="150px" />
					<column label="Tipo Historia" width="150px" />
					<column label="Acciones" align="center"
						width="80px" />
				</columns>
				<rows id="rowsResultado" width="100%" />
				</grid>
				</groupbox>
				
				<groupbox id="groupboxEditar" visible="true" closable="false"
					mold="3d" height="100%">
					<caption label="Registrar/Modificar Historias Clínicas"
						style="font-weight: bold">
						<toolbarbutton image="/images/add_perfil.png"
