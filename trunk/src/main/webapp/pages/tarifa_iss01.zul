<?page title="Tarifa_iss01" contentType="text/html;charset=UTF-8"?>
<zk xmlns:h="http://www.w3.org/1999/xhtml">
	<window id="formTarifa_iss01" width="100%" height="97%"
		use="healthmanager.controller.Tarifa_iss01Action">
		<style>
			body ,table{ margin:0; padding:0; } .z-fieldset{
			-moz-border-radius:5px; border-radius: 5px;
			-webkit-border-radius: 5px; } .combobox{ font-size:12px;
			border: 1px solid silver; } tr.z-row .z-cell{
			background-image:none; } tr.z-row td.z-row-hover{
			background-image:none; } .GridSinBorde tr.z-row
			td.z-row-inner, tr.z-row .z-cell,div.z-grid { border: none;
			overflow: hidden; zoom: 1; border-top: none; border-left:
			none; border-right: none; border-bottom: none; }
		</style>


		<!-- **************** -->
		<!-- Zona de Consulta -->
		<!-- **************** -->
		<groupbox id="groupboxConsulta" visible="true" closable="false"
			mold="3d" height="90%">
			<caption label="Consultar"
				style="color: blue;font-weight: bold" />

			<separator />

			<!-- ***************************** -->
			<!--  Resultado Consulta Maestros  -->
			<!-- ***************************** -->
			<grid id="gridResultado" height="90%" mold="paging"
				pageSize="20" vflex="1">
				<auxhead>
					<auxheader colspan="4">
						<hbox align="center">
							<label value="Criterios de B&#250;squeda :" />
							<space />
							<listbox id="lbxParameter" mold="select"
								sclass="combobox" width="150px" />
							<separator />
							<textbox id="tbxValue" width="200px"
								onOK="formTarifa_iss01.buscarDatosTarifa()"
								style="text-transform:uppercase" />
							<separator />
							<button mold="trendy"
								image="/images/Magnifier.gif"
								onClick="formTarifa_iss01.buscarDatosTarifa()"
								tooltiptext="Consultar Tarifa iss01" />
							<separator />
							<button mold="trendy"
								image="/images/New16.gif"
								onClick='formTarifa_iss01.accionForm(true,"registrar");'
								tooltiptext="Nuevo Tarifa iss01" />
						</hbox>
					</auxheader>
				</auxhead>
				<columns sizable="true">
					<column label="Codigo pcd" />
					<column label="Descripcion" />
					<column label="Valor" />
					<column label="Acciones" align="center"
						width="80px" />
				</columns>
				<rows id="rowsResultado" width="100%" />
			</grid>
		</groupbox>

		<borderlayout width="100%" height="90%" id="groupboxEditar"
			visible="false">
			<center autoscroll="true" border="none">
				<div align="center">

					<!-- *************************** -->
					<!-- *** Insertar/Actualizar *** -->
					<!-- *************************** -->
					<groupbox closable="false" width="920px"
						mold="3d">
						<grid sclass="GridSinBorde" hflex="1">
							<columns>
								<column label="" width="276px" />
								<column label="" width="170px"
									align="left" />
								<column label="" width="182px" />
								<column label="" width="276px" />
							</columns>
							<rows>

								<!-- fila 1 -->
								<row visible="false"> 
									<cell></cell>
									<cell height="30px">
										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
										<label
											id="lbCodigo_administradora" value="Codigo administradora: " />
									</cell>
									<cell>
										<listbox
											id="lbxCodigo_administradora" name="codigo_administradora"
											mold="select" width="170px" sclass="combobox" />
									</cell>
									<cell></cell>
								</row>

								<!-- fila 2 -->
								<row visible="false">
									<cell></cell>
									<cell height="30px">
										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
										<label id="lbId_plan"
											value="Id plan: " />
									</cell>
									<cell>
										<textbox id="tbxId_plan"
											width="170px" style="text-transform:uppercase"
											maxlength="20" />
									</cell>
									<cell></cell>
								</row>

								<!-- fila 3 -->
								<row>
									<cell></cell>
									<cell height="30px">
										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
										<label id="lbCodigo_pcd"
											value="Codigo pcd: " />
									</cell>
									<cell>
										<bandbox id="tbxCodigo_pcd" 
											width="170px" style="text-transform:uppercase" readonly="true"
											maxlength="10" onOpen="formTarifa_iss01.openPcd(healthmanager.controller.PlanesAction.TypeProce.ISS01)"/>
									</cell>
									<cell></cell>
								</row>
								
								<row>
									<cell></cell>
									<cell height="30px">
										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
										<label id="lbDescripcion"
											value="Descripcion: " />
									</cell>
									<cell>
										<textbox id="tbxDescripcion"
											width="170px" style="text-transform:uppercase"
											maxlength="100" />
									</cell>
									<cell></cell>
								</row>

								<!-- fila 4 -->
								<row>
									<cell></cell>
									<cell height="30px">
										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
										<label id="lbValor"
											value="Valor: " />
									</cell>
									<cell>
										<doublebox id="dbxValor"
											format="#,##0.00" width="170px" value="0.00" />
									</cell>
									<cell></cell>
								</row>

								<!-- fila 5 -->
								<row visible="false">
									<cell></cell>
									<cell height="30px">
										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
										<label id="lbDelete_date"
											value="Delete date: " />
									</cell>
									<cell>
										<datebox id="dtbxDelete_date"
											onCreate="self.value = new Date();" format="yyyy-MM-dd"
											width="170px" />
									</cell>
									<cell></cell>
								</row>

								<!-- fila 6 -->
								

							</rows>
							<foot>
								<footer span="8">
									<div align="center">
										<textbox id="tbxAccion"
											value="registrar" visible="false" />
										<toolbarbutton
											image="/images/Save16.gif" label="Guardar Tarifa iss01"
											onClick="formTarifa_iss01.guardarDatos();" id="btGuardar" />
										<toolbarbutton
											image="/images/New16.gif" label="Nueva Tarifa iss01"
											onClick='formTarifa_iss01.accionForm(true,"registrar");'
											id="btNew" />
										<space />
										<toolbarbutton
											image="/images/trasladar.gif" label="Cancelar"
											onClick='formTarifa_iss01.accionForm(false,tbxAccion.getText());'
											id="btCancel" />
										<toolbarbutton
											image="/images/print_ico.gif" label="Imprimir Tarifa iss01"
											onClick="formTarifa_iss01.imprimir();" id="btImprimir"
											visible="false" />
										<space />
										<label
											value="Formato impresiÃ³n: " visible="false" />
										<listbox id="lbxFormato"
											mold="select" width="150px" visible="false"
											sclass="combobox">
											<listitem value="pdf"
												label="PDF" selected="true" />
											<listitem value="rtf"
												label="WORD" />
										</listbox>
									</div>
								</footer>
							</foot>
						</grid>

					</groupbox>
				</div>
			</center>
		</borderlayout>
	</window>
</zk>