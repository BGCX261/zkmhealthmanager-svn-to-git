								<column label="" align="left" />
								<column label="" width="130px"
									align="left" />
								<column label="" align="left" />
								<column label="" width="5px"
									align="left" />
							</columns>
							<rows>
								<row>
									<cell />
									<cell height="30px">
										<label
											value="Copiar A침o: " />
									</cell>
									<cell>
										<listbox id="lbxAnioCopiar"
											mold="select" hflex="1" sclass="combobox" />
									</cell>
									<cell height="30px">
										<label
											value="Copiar Mes: " />
									</cell>
									<cell>
										<listbox id="lbxMesCopiar"
											mold="select" hflex="1" sclass="combobox" />
									</cell>
									<cell />
								</row>
								<row>
									<cell />
									<cell height="30px">
										<label value="A침o: " />
									</cell>
									<cell>
