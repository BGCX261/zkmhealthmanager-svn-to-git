											<radio
												label="Consumidor de sustancias psicoactivas" value="1"
												radiogroup="rdbFactor_de_vulnerabilidad" />

										</cell>
										<cell>
											<radio label="Maestro"
												value="1" radiogroup="rdbFactor_de_vulnerabilidad" />

										</cell>
									</row>
									<row>
										<cell>
											<radio
												label="Tribus urbanas" value="1"
												radiogroup="rdbFactor_de_vulnerabilidad" />

										</cell>
										<cell>
											<radio label="Reciclador"
												value="1" radiogroup="rdbFactor_de_vulnerabilidad" />

										</cell>
										<cell>
											<radio
												label="Defensor(a) de los derechos humanos" value="1"
												radiogroup="rdbFactor_de_vulnerabilidad" />

										</cell>
										<cell>
											<radio
