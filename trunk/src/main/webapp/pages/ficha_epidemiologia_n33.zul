										<cell>
											<checkbox
												id="chbViolencia_psicologia" />
										</cell>
									</row>
								</rows>
							</grid>
						</groupbox>
						<groupbox mold="3d" closable="false">
							<caption label="NATURALEZA DE LA VIOLENCIA"
								style="font-weight:bold" />
							<grid>
								<columns>
									<column width="26%" />
									<column width="20%" />
									<column width="" />
									<column width="17%" />

								</columns>
								<rows>
