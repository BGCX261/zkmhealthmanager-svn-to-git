										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
										<label
											id="lbCodigo_administradora" value="Codigo administradora: " />
									</cell>
									<cell>
										<textbox
											id="tbxCodigo_administradora" width="170px"
											style="text-transform:uppercase" maxlength="6" />
									</cell>
									<cell></cell>
								</row>

								<!-- fila 3 -->
								<row>
									<cell></cell>
									<cell height="30px">
										<label value="* "
											style="color: red" tooltiptext="Campo obligatorio" />
